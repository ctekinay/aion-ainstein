:62
  a                 skos:Concept ;
  skos:prefLabel    "Interconnectorsysteem"@nl ;
  skos:definition   "Interconnectorsysteem voor elektriciteit of voor gas"@nl ;
  dct:identifier    "urn:uuid:09f3d184-92a2-4461-873c-6feb92143765" ;
  skos:related      :59 ;
