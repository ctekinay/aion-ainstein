  owl:versionInfo   "1" ;
  dct:created       "2025-07-13"^^xsd:date ;
  dct:identifier    "677d480a-4d07-57fb-9788-a400cf9e655c" ;
  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

:2264
  a                 skos:Concept ;
  skos:prefLabel    "Productie-eenheid"@nl ;
  skos:definition   "Een productie-eenheid of elektriciteitsproductie-eenheid (ofwel power generating module, afgekort PGM) is een groep synchroon gekoppelde productieinstallaties, bijvoorbeeld een wind- of zonnepark. Een PGM met een capaciteit van 1 MW tot 50 MW is type B, een PGM met een capaciteit van 50 MW tot 60 MW is type C. Voor beide gevallen moet de klant een PGMD aanleveren bij de netbeheerder."@nl ;
  owl:versionInfo   "1" ;
  dct:created       "2025-07-13"^^xsd:date ;
  dct:identifier    "374ecac3-3e65-5f26-b8ef-fbab56155183" ;
  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

:2265
  a                 skos:Concept ;
  skos:prefLabel    "Productie-eenheid"@nl ;
  skos:definition   "De installatie, waarin vergisting van organisch materiaal plaatsvindt en waar alle overige maatregelen worden getroffen om een veilige en betrouwbare levering van ruw gas te kunnen realiseren."@nl ;
