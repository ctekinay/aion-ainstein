  dcat:theme        "vocabulary"@en .

:2195-1
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The function of operating the algorithm applied to optimise the activation of balancing energy bids."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Activation optimisation function"@en .

:2195-10
  a               skos:Concept ;
  dc:source       "EU 2017/2195" ;
  skos:definition "Balancing energy or balancing capacity, or both."@en ;
  skos:inScheme   :Ontology ;
  skos:prefLabel  "Balancing services"@en .

:2195-11
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The function of operating the algorithm applied for the optimisation of the procurement of balancing capacity for TSOs exchanging balancing capacity."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Capacity procurement optimisation function"@en .

:2195-12
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "A scheduling and dispatching model where the generation schedules and consumption schedules as well as dispatching of power generating facilities and demand facilities, in reference to dispatchable facilities, are determined by a TSO within the integrated scheduling process."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Central dispatching model"@en .

:2195-13
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "A list of balancing energy bids sorted in order of their bid prices, used for the activation of those bids."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Common merit order list"@en .

:2195-14
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The TSO that operates the scheduling area in which balancing service providers and balance responsible parties shall be compliant with the terms and conditions related to balancing."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Connecting TSO"@en .

:2195-15
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The TSO that has contractual arrangements for balancing services with a balancing service provider in another scheduling area."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Contracting TSO"@en .

:2195-16
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The period for ramping from full delivery to a set point, or from full withdrawal back to a set point."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Deactivation period"@en .

:2195-17
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The period of delivery during which the balancing service provider delivers the full requested change of power in-feed to, or the full requested change of withdrawals from the system."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Delivery period"@en .

:2195-18
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The possibility for a TSO to use only part of the balancing energy bids or balancing capacity bids offered by the balancing service provider, either in terms of power activation or time duration."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Divisibility"@en .

:2195-19
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The provision of balancing capacity to a TSO in a different scheduling area than the one in which the procured balancing service provider is connected."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Exchange of balancing capacity"@en .

:2195-2
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "An energy volume physically injected or withdrawn from the system and attributed to a balance responsible party, for the calculation of the imbalance of that balance responsible party."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Allocated volume"@en .

:2195-20
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The activation of balancing energy bids for the delivery of balancing energy to a TSO in a different scheduling area than the one in which the activated balancing service provider is connected."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Exchange of balancing energy"@en .

:2195-21
  a               skos:Concept ;
  dc:source       "EU 2017/2195" ;
  skos:definition "Either or both exchange of balancing energy and exchange of balancing capacity."@en ;
  skos:inScheme   :Ontology ;
  skos:prefLabel  "Exchange of balancing services"@en .

:2195-22
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The period between the activation request by the connecting TSO in case of TSO-TSO model or by the contracting TSO in case of TSO-BSP model and the corresponding full delivery of the concerned product."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Full activation time"@en .

:2195-23
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "An energy volume calculated for a balance responsible party and representing the difference between the allocated volume attributed to that balance responsible party and the final position of that balance responsible party, including any imbalance adjustment applied to that balance responsible party, within a given imbalance settlement period."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Imbalance"@en .

:2195-24
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "An energy volume representing the balancing energy from a balancing service provider and applied by the connecting TSO for an imbalance settlement period to the concerned balance responsible parties, used for the calculation of the imbalance of these balance responsible parties."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Imbalance adjustment"@en .

:2195-25
  a               skos:Concept ;
  dc:source       "EU 2017/2195" ;
  skos:definition "The area in which an imbalance is calculated."@en ;
  skos:inScheme   :Ontology ;
  skos:prefLabel  "Imbalance area"@en .

:2195-26
  a               skos:Concept ;
  dc:source       "EU 2017/2195" ;
  skos:definition "The role to operate the algorithm applied for operating the imbalance netting process."@en ;
  skos:inScheme   :Ontology ;
  skos:prefLabel  "Imbalance netting process function"@en .

:2195-27
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The price, be it positive, zero or negative, in each imbalance settlement period for an imbalance in each direction."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Imbalance price"@en .

:2195-28
  a               skos:Concept ;
  dc:source       "EU 2017/2195" ;
  skos:definition "The area for the calculation of an imbalance price."@en ;
  skos:inScheme   :Ontology ;
  skos:prefLabel  "Imbalance price area"@en .

:2195-29
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "A financial settlement mechanism for charging or paying balance responsible parties for their imbalances."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Imbalance settlement"@en .

:2195-3
  a               skos:Concept ;
  dc:source       "EU 2017/2195" ;
  skos:definition "A market participant or its chosen representative responsible for its imbalances."@en ;
  skos:inScheme   :Ontology ;
  skos:prefLabel  "Balance responsible party"@en .

:2195-30
  a               skos:Concept ;
  dc:source       "EU 2017/2195" ;
  skos:definition "The time unit for which balance responsible parties' imbalance is calculated."@en ;
  skos:inScheme   :Ontology ;
  skos:prefLabel  "Imbalance settlement period"@en .

:2195-31
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "An iterative process that uses at least integrated scheduling process bids that contain commercial data, complex technical data of individual power generating facilities or demand facilities and explicitly includes the start-up characteristics, the latest control area adequacy analysis and the operational security limits as an input to the process."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Integrated scheduling process"@en .

:2195-32
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The point in time when the submission or the update of integrated scheduling process bids is no longer permitted for the given iterations of the integrated scheduling process."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Integrated scheduling process gate closure time"@en .

:2195-33
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The mode of activation of balancing energy bids, manual or automatic, depending on whether balancing energy is triggered manually by an operator or automatically in a closed-loop manner."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Mode of activation"@en .

:2195-34
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The declared energy volume of a balance responsible party used for the calculation of its imbalance."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Position"@en .

:2195-35
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The period between the request by the connecting TSO in case of TSO-TSO model or by the contracting TSO in case of TSO-BSP model and the start of the ramping period."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Preparation period"@en .

:2195-36
  a               skos:Concept ;
  dc:source       "EU 2017/2195" ;
  skos:definition "The TSO that requests the delivery of balancing energy."@en ;
  skos:inScheme   :Ontology ;
  skos:prefLabel  "Requesting TSO"@en .

:2195-37
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "A scheduling and dispatching model where the generation schedules and consumption schedules as well as dispatching of power generating facilities and demand facilities are determined by the scheduling agents of those facilities."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Self-dispatching model"@en .

:2195-38
  a               skos:Concept ;
  dc:source       "EU 2017/2195" ;
  skos:definition "A product different from a standard product."@en ;
  skos:inScheme   :Ontology ;
  skos:prefLabel  "Specific product"@en .

:2195-39
  a               skos:Concept ;
  dc:source       "EU 2017/2195" ;
  skos:definition "A harmonised balancing product defined by all TSOs for the exchange of balancing services."@en ;
  skos:inScheme   :Ontology ;
  skos:prefLabel  "Standard product"@en .

:2195-4
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "All actions and processes, on all timelines, through which TSOs ensure, in a continuous way, the maintenance of system frequency within a predefined stability range as set out in Article 127 of Regulation (EU) 2017/1485, and compliance with the amount of reserves needed with respect to the required quality, as set out in Part IV Title V, Title VI and Title VII of Regulation (EU) 2017/1485."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Balancing"@en .

:2195-40
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "A transfer of balancing capacity from the initially contracted balancing service provider to another balancing service provider."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Transfer of balancing capacity"@en .

:2195-41
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The latest point in time when a connecting TSO can forward the balancing energy bids received from a balancing service provider to the activation optimisation function."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "TSO energy bid submission gate closure time"@en .

:2195-42
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "A model for the exchange of balancing services where the balancing service provider provides balancing services directly to the contracting TSO, which then provides these balancing services to the requesting TSO."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "TSO-BSP model"@en .

:2195-43
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "A model for the exchange of balancing services where the balancing service provider provides balancing services to its connecting TSO, which then provides these balancing services to the requesting TSO."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "TSO-TSO model"@en .

:2195-44
  a               skos:Concept ;
  dc:source       "EU 2017/2195" ;
  skos:definition "The function of performing the settlement of cooperation processes between the TSOs."@en ;
  skos:inScheme   :Ontology ;
  skos:prefLabel  "TSO-TSO settlement function"@en .

:2195-45
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The period when the balancing energy bid offered by the balancing service provider can be activated, where all the characteristics of the product are respected. The validity period is defined by a start time and an end time."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Validity period"@en .

:2195-5
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "A volume of reserve capacity that a balancing service provider has agreed to hold and in respect to which the balancing service provider has agreed to submit bids for a corresponding volume of balancing energy to the TSO for the duration of the contract."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Balancing capacity"@en .

:2195-6
  a               skos:Concept ;
  dc:source       "EU 2017/2195" ;
  skos:definition "Energy used by TSOs to perform balancing and provided by a balancing service provider."@en ;
  skos:inScheme   :Ontology ;
  skos:prefLabel  "Balancing energy"@en .

:2195-7
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The point in time when submission or update of a balancing energy bid for a standard product on a common merit order list is no longer permitted."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Balancing energy gate closure time"@en .

:2195-8
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "The entirety of institutional, commercial and operational arrangements that establish market-based management of balancing."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Balancing market"@en .

:2195-9
  a              skos:Concept ;
  dc:source      "EU 2017/2195" ;
  skos:definition
                 "A market participant with reserve-providing units or reserve-providing groups able to provide balancing services to TSOs."@en ;
  skos:inScheme  :Ontology ;
  skos:prefLabel "Balancing service provider"@en .

# Definition and abbreviations (2022/2555)

:2555
  a                 skos:Collection ;
  skos:prefLabel    "2555 - Measures for a high common level of cybersecurity"@en,
                    "2555 - Maatregelen voor een hoog gezamenlijk niveau van cyberbeveiliging"@nl ;
  skos:inScheme     :Ontology ;
  dc:source         "32022L2555"@en ;
  skos:member       :2555-1, :2555-2, :2555-3, :2555-4, :2555-5, :2555-6, :2555-7, :2555-8, :2555-9, :2555-10, :2555-11,
                    :2555-12, :2555-13, :2555-14, :2555-15, :2555-16, :2555-17, :2555-18, :2555-19, :2555-20, :2555-21,
                    :2555-22, :2555-23, :2555-24, :2555-25, :2555-26, :2555-27, :2555-28, :2555-29, :2555-30, :2555-31,
                    :2555-32, :2555-33, :2555-34, :2555-35, :2555-36, :2555-37, :2555-38, :2555-39, :2555-40, :2555-41 ;
