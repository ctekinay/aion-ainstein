  dct:identifier    "6736020d-9e90-513e-9e74-131f8cf68ede" ;
  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

:307
  a                 skos:Concept ;
  skos:prefLabel    "Beheren"@nl ;
  skos:definition   "Alle activiteiten gericht op het behouden van data in een bruikbare staat."@nl ;
