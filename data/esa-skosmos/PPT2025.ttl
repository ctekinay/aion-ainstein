  skos:topConceptOf :Ontology .

:302
  a                 skos:Concept ;
  skos:prefLabel    "Begrippenmodel"@nl ;
  skos:definition   "Beschrijft de werkelijkheid binnen het beschouwde domein (de ‘universe of discourse’) d.m.v. de daarin gehanteerde begrippen en hun relaties tot elkaar. Doel is dat de actoren daarbinnen elkaar begrijpen en één taal spreken. Een model van begrippen wordt opgesteld voor gebruik door mensen, met name ‘de business’."@nl ;
  owl:versionInfo   "1" ;
  dct:created       "2025-07-13"^^xsd:date ;
  dct:identifier    "fb2483e2-c3d5-57bb-9b5d-a32ce17f9f4b" ;
  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

:303
  a                 skos:Concept ;
  skos:prefLabel    "Begrippenmodel"@nl ;
  skos:definition   "Beschrijft de werkelijkheid binnen het beschouwde domein (de ‘universe of discourse’) d.m.v. de daarin gehanteerde begrippen en hun relaties tot elkaar. Doel is dat de actoren daarbinnen elkaar begrijpen en één taal spreken. Een model van begrippen wordt opgesteld voor gebruik door mensen, met name uit ‘de business’. De begrippen worden beschreven in een formele taal, een vocabulaire. Een vocabulaire is geen informatiemodel. Begrippen kunnen in meerdere informatiemodellen gebruikt worden. "@nl ;
  owl:versionInfo   "1" ;
  dct:created       "2025-07-13"^^xsd:date ;
  dct:identifier    "b734833e-1236-59c6-8c2f-849ebcf0bee4" ;
  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

:304
  a                 skos:Concept ;
  skos:prefLabel    "Beheer doelstelling"@nl ;
  skos:definition   "Managementdoelstellingen die worden gebruikt als leidraad om beheersinstrumenten te ontwikkelen en te implementeren."@nl ;
  owl:versionInfo   "1" ;
  dct:created       "2025-07-13"^^xsd:date ;
  dct:identifier    "78e28e97-552c-5333-81c6-97fb675876d8" ;
  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

:305
  a                 skos:Concept ;
  skos:prefLabel    "Beheersmaatregel"@nl ;
