  dct:identifier    "urn:uuid:6bdb0623-dd23-5b6e-b946-cd9d56dd18e8" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:157
  a                 skos:Concept ;
  skos:prefLabel    "Risicogebied"@nl ;
  skos:definition   "Deelgebied in het voor een netbeheerder krachtens artikel 36of 37 van de Elektriciteitswet 1998vastgestelde gebied waar het risico op lichamelijk letsel ten gevolge van een kortsluiting in het laagspanningsnet groter is dan in de rest van het voor hem vastgestelde gebied;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:fa63c798-460e-592f-8ddb-b7acc9b66731" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:158
  a                 skos:Concept ;
  skos:prefLabel    "SAP"@nl ;
  skos:definition   "All TSOs’ proposal for the establishment of a single allocation Platform (SAP) in accordance with Article 49 and for the cost sharing methodology in accordance with Article 59 of Commission Regulation (EU) 2016/1719 establishing a Guideline on Forward Capacity allocation, zoals goedgekeurd door de Autoriteit Consument en Markt (hierna: de ACM) bij besluit ACM/DE/2017/205795 van 17 oktober 2017;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:16b55621-8e67-5506-8caa-2548085a7e82" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:159
  a                 skos:Concept ;
  skos:prefLabel    "Schakelstoring"@nl ;
  skos:definition   "Een storing in de meetinrichting waardoor de aansturing van de telwerken niet heeft plaatsgevonden op de bij het profiel behorende tijden."@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:aaac962f-4937-5a55-ac35-8ad47526d932" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:160
