  dct:identifier    "502b92bf-60d6-579a-b5a6-4cff13380199" ;
  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

:2262
  a                 skos:Concept ;
  skos:prefLabel    "Productie-eenheid"@nl ;
  skos:definition   "Een (deel van een) productie-installatie, dat zelfstandig (ofwel onafhankelijk van de andere delen van het desbetreffende productie-installatie) kan worden ingezet voor het opwekken van elektriciteit. Van een productie-eenheid kunnen één of meer generatoren deel uitmaken."@nl ;
  owl:versionInfo   "1" ;
  dct:created       "2025-07-13"^^xsd:date ;
  dct:identifier    "8f933361-85bd-5dcf-8876-4c000a0ad2a2" ;
