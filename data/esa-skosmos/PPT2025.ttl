  dct:identifier    "96af3a91-5084-5044-a79c-c67af22fbb18" ;
  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

:2267
  a                 skos:Concept ;
  skos:prefLabel    "Productie-installatie"@nl ;
