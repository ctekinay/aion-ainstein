  dct:identifier    "urn:uuid:64f4cc1b-bcbb-5c10-b04d-5cbb1edea5c9" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:151
  a                 skos:Concept ;
  skos:prefLabel    "Regionale netbeheerder"@nl ;
  skos:definition   "Een netbeheerder die is aangewezen voor het beheer van één of meer netten, anders dan het landelijk hoogspanningsnet;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:e8730ab3-79ac-5aa7-aef4-c9a2313d3de6" ;
