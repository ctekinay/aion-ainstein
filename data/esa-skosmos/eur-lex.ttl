  dcat:theme        "vocabulary"@en .


:2555-1
  a               skos:Concept ;
  skos:prefLabel  "Network and information system"@en ;
  skos:definition """(a) An electronic communications network as defined in Article 2, point (1), of Directive (EU) 2018/1972;
(b) Any device or group of interconnected or related devices, one or more of which, pursuant to a programme, carry out automatic processing of digital data;
(c) Digital data stored, processed, retrieved or transmitted by elements covered under points (a) and (b) for the purposes of their operation, use, protection and maintenance."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-2
  a              skos:Concept ;
  skos:prefLabel "Security of network and information systems"@en ;
  skos:definition
                 """The ability of network and information systems to resist, at a given level of confidence, any event that may compromise the availability, authenticity, integrity or confidentiality of stored, transmitted or processed data or of the services offered by, or accessible via, those network and information systems."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-3
  a               skos:Concept ;
  skos:prefLabel  "Cybersecurity"@en ;
  skos:definition """Cybersecurity as defined in Article 2, point (1), of Regulation (EU) 2019/881."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-4
  a              skos:Concept ;
  skos:prefLabel "National cybersecurity strategy"@en ;
  skos:definition
                 """A coherent framework of a Member State providing strategic objectives and priorities in the area of cybersecurity and the governance to achieve them in that Member State."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-5
  a              skos:Concept ;
  skos:prefLabel "Near miss"@en ;
  skos:definition
                 """An event that could have compromised the availability, authenticity, integrity or confidentiality of stored, transmitted or processed data or of the services offered by, or accessible via, network and information systems, but that was successfully prevented from materialising or that did not materialise."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-6
  a              skos:Concept ;
  skos:prefLabel "Incident"@en ;
  skos:definition
                 """An event compromising the availability, authenticity, integrity or confidentiality of stored, transmitted or processed data or of the services offered by, or accessible via, network and information systems."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-7
  a              skos:Concept ;
  skos:prefLabel "Large-scale cybersecurity incident"@en ;
  skos:definition
                 """An incident which causes a level of disruption that exceeds a Member State’s capacity to respond to it or which has a significant impact on at least two Member States."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-8
  a              skos:Concept ;
  skos:prefLabel "Incident handling"@en ;
  skos:definition
                 """Any actions and procedures aiming to prevent, detect, analyse, and contain or to respond to and recover from an incident."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-9
  a              skos:Concept ;
  skos:prefLabel "Risk"@en ;
  skos:definition
                 """The potential for loss or disruption caused by an incident and is to be expressed as a combination of the magnitude of such loss or disruption and the likelihood of occurrence of the incident."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-10
  a               skos:Concept ;
  skos:prefLabel  "Cyber threat"@en ;
  skos:definition """A cyber threat as defined in Article 2, point (8), of Regulation (EU) 2019/881."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-11
  a              skos:Concept ;
  skos:prefLabel "Significant cyber threat"@en ;
  skos:definition
                 """A cyber threat which, based on its technical characteristics, can be assumed to have the potential to have a severe impact on the network and information systems of an entity or the users of the entity’s services by causing considerable material or non-material damage."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-12
  a               skos:Concept ;
  skos:prefLabel  "ICT product"@en ;
  skos:definition """An ICT product as defined in Article 2, point (12), of Regulation (EU) 2019/881."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-13
  a               skos:Concept ;
  skos:prefLabel  "ICT service"@en ;
  skos:definition """An ICT service as defined in Article 2, point (13), of Regulation (EU) 2019/881."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-14
  a               skos:Concept ;
  skos:prefLabel  "ICT process"@en ;
  skos:definition """An ICT process as defined in Article 2, point (14), of Regulation (EU) 2019/881."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-15
  a              skos:Concept ;
  skos:prefLabel "Vulnerability"@en ;
  skos:definition
                 """A weakness, susceptibility or flaw of ICT products or ICT services that can be exploited by a cyber threat."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-16
  a              skos:Concept ;
  skos:prefLabel "Standard"@en ;
  skos:definition
                 """A standard as defined in Article 2, point (1), of Regulation (EU) No 1025/2012 of the European Parliament and of the Council."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-17
  a              skos:Concept ;
  skos:prefLabel "Technical specification"@en ;
  skos:definition
                 """A technical specification as defined in Article 2, point (4), of Regulation (EU) No 1025/2012."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-18
  a              skos:Concept ;
  skos:prefLabel "Internet exchange point"@en ;
  skos:definition
                 """A network facility which enables the interconnection of more than two independent networks (autonomous systems), primarily for the purpose of facilitating the exchange of internet traffic."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-19
  a              skos:Concept ;
  skos:prefLabel "Domain name system"@en ;
  skos:definition
                 """A hierarchical distributed naming system which enables the identification of internet services and resources."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-20
  a              skos:Concept ;
  skos:prefLabel "DNS service provider"@en ;
  skos:definition
                 """An entity that provides publicly available recursive or authoritative domain name resolution services."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-21
  a               skos:Concept ;
  skos:prefLabel  "Top-level domain name registry"@en ;
  skos:definition """An entity responsible for administering a TLD and its technical operations."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-22
  a               skos:Concept ;
  skos:prefLabel  "Entity providing domain name registration services"@en ;
  skos:definition """A registrar or agent acting on behalf of registrars."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-23
  a               skos:Concept ;
  skos:prefLabel  "Digital service"@en ;
  skos:definition """A service as defined in Article 1(1), point (b), of Directive (EU) 2015/1535."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-24
  a               skos:Concept ;
  skos:prefLabel  "Trust service"@en ;
  skos:definition """A trust service as defined in Article 3, point (16), of Regulation (EU) No 910/2014."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-25
  a               skos:Concept ;
  skos:prefLabel  "Trust service provider"@en ;
  skos:definition """A trust service provider as defined in Article 3, point (19), of Regulation (EU) No 910/2014."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-26
  a              skos:Concept ;
  skos:prefLabel "Qualified trust service"@en ;
  skos:definition
                 """A qualified trust service as defined in Article 3, point (17), of Regulation (EU) No 910/2014."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-27
  a              skos:Concept ;
  skos:prefLabel "Qualified trust service provider"@en ;
  skos:definition
                 """A qualified trust service provider as defined in Article 3, point (20), of Regulation (EU) No 910/2014."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-28
  a               skos:Concept ;
  skos:prefLabel  "Online marketplace"@en ;
  skos:definition """An online marketplace as defined in Article 2, point (n), of Directive 2005/29/EC."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-29
  a               skos:Concept ;
  skos:prefLabel  "Online search engine"@en ;
  skos:definition """An online search engine as defined in Article 2, point (5), of Regulation (EU) 2019/1150."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-30
  a              skos:Concept ;
  skos:prefLabel "Cloud computing service"@en ;
  skos:definition
                 """A digital service that enables on-demand administration and broad remote access to a scalable and elastic pool of shareable computing resources."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-31
  a              skos:Concept ;
  skos:prefLabel "Data centre service"@en ;
  skos:definition
                 """A service encompassing structures dedicated to the centralised accommodation and operation of IT and network equipment."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-32
  a              skos:Concept ;
  skos:prefLabel "Content delivery network"@en ;
  skos:definition
                 """A network of geographically distributed servers for fast delivery of digital content and services."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-33
  a               skos:Concept ;
  skos:prefLabel  "Social networking services platform"@en ;
  skos:definition """A platform enabling users to connect, share, discover and communicate."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-34
  a              skos:Concept ;
  skos:prefLabel "Representative"@en ;
  skos:definition
                 """A person established in the EU designated to act on behalf of certain non-EU service providers."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-35
  a               skos:Concept ;
  skos:prefLabel  "Public administration entity"@en ;
  skos:definition """An entity recognised in a Member State as a public administration according to national law."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-36
  a              skos:Concept ;
  skos:prefLabel "Public electronic communications network"@en ;
  skos:definition
                 """A public electronic communications network as defined in Article 2, point (8), of Directive (EU) 2018/1972."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-37
  a              skos:Concept ;
  skos:prefLabel "Electronic communications service"@en ;
  skos:definition
                 """An electronic communications service as defined in Article 2, point (4), of Directive (EU) 2018/1972."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .

:2555-38
  a               skos:Concept ;
  skos:prefLabel  "Entity"@en ;
  skos:definition """A natural or legal person created and recognised under national law."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-39
  a               skos:Concept ;
  skos:prefLabel  "Managed service provider"@en ;
  skos:definition """An entity providing installation, management, or maintenance of ICT products or systems."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-40
  a               skos:Concept ;
  skos:prefLabel  "Managed security service provider"@en ;
  skos:definition """A managed service provider handling cybersecurity risk management."""@en ;
  dc:source       "EU 2022/2555" ;
  skos:inScheme   :Ontology .

:2555-41
  a              skos:Concept ;
  skos:prefLabel "Research organisation"@en ;
  skos:definition
                 """An entity conducting applied research for commercial purposes, excluding educational institutions."""@en ;
  dc:source      "EU 2022/2555" ;
  skos:inScheme  :Ontology .
