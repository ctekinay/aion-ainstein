  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

:2255
  a                 skos:Concept ;
  skos:prefLabel    "Producent"@nl ;
  skos:definition   "De producent wint gas of wekt energie op en verkoopt dit door. Het opwekken gebeurt door middel van kerncentrales, gas en steenkool, windmolens, zonnepanelen, biomassa en waterkrachtcentrales."@nl ;
  owl:versionInfo   "1" ;
  dct:created       "2025-07-13"^^xsd:date ;
  dct:identifier    "f87f0d2e-8187-5e53-84cb-36d3f8b39a11" ;
  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

:2256
  a                 skos:Concept ;
  skos:prefLabel    "Producent"@nl ;
