  skos:inScheme     :Ontology .

:91
  a                 skos:Concept ;
  skos:prefLabel    "Meetmethode"@nl ;
  skos:definition   "De aanduiding in het aansluitingenregister van de wijze waarop de energie-uitwisseling op de aansluiting wordt vastgesteld;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:0b4a8c9e-3586-5904-ad8c-e21a1d56a6da" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:92
  a                 skos:Concept ;
  skos:prefLabel    "Meetmiddel (van de meetinrichting)"@nl ;
  skos:definition   "Een kWh-meter, kvarh-meter, stroom- of spanningstransformator;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:35dcff89-e89e-506d-a40e-a94c5fd9286b" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:93
  a                 skos:Concept ;
  skos:prefLabel    "Meetverantwoordelijke"@nl ;
  skos:definition   "Een persoon die meetverantwoordelijkheid uitoefent en die daarvoor conform bijlage 4 van de Meetcode elektriciteiten/of bijlage 3 van de Meetcode gas RNBis erkend;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:2c2b5f90-2347-5184-8ce2-1b3d4d571ae1" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:94
  a                 skos:Concept ;
  skos:prefLabel    "Meetverantwoordelijkheid"@nl ;
  skos:definition   "De verantwoordelijkheid van de aangeslotene voor het aanwezig zijn op de aansluiting van een op grond van hoofdstuk 2 van de Meetcode elektriciteitvereiste meetinrichting, meetinrichtingen in het kader van de eigen opwekking en meetinrichtingen in het kader van de Wet milieukwtermiteit elektriciteitsproductie, alsmede voor het correct en tijdig (doen) vaststellen en (doen) doorgeven van meetgegevens op grond van hoofdstuk 6 van de Informatiecode elektriciteit en gas;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:142f6141-ae74-5014-83e4-df49173895e2" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:95
  a                 skos:Concept ;
  skos:prefLabel    "Meterbeheerder"@nl ;
  skos:definition   "Een organisatorische eenheid die verantwoordelijk is voor ontwerp, plaatsing, beheer, onderhoud en verwijdering van de meetinrichting;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:b01dc05c-46c5-5ffc-857c-1a7192d14911" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:96
  a                 skos:Concept ;
  skos:prefLabel    "Meterplaatser"@nl ;
  skos:definition   "Een persoon, niet zijnde een netbeheerder, die conform artikel 26ad, lid 6of artikel 26ae, lid 7 van de Elektriciteitswet 1998of artikel 13d, lid 6, of artikel 13e, lid 7, van de Gaswet, op verzoek van een kleinverbruiker een meetinrichting plaatst bij een kleinverbruikaansluiting en die daarvoor conform bijlage 4 van de Meetcode elektriciteiten/of bijlage 3 van de Meetcode gas RNBis erkend;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:45d4ccde-a5e2-5729-b92a-6b09fca1c283" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:97
  a                 skos:Concept ;
  skos:prefLabel    "Middenspanningsnet"@nl ;
  skos:definition   "Net bestemd voor het transport van elektriciteit op een spanningsniveau groter dan 1 kV, maar kleiner dan of gelijk aan 35 kV en dat als zodanig wordt bedreven."@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:7059d134-5562-5444-b429-b35dcf2cd045" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:98
  a                 skos:Concept ;
  skos:prefLabel    "MP-erkenner"@nl ;
  skos:definition   "De instantie die conform bijlage 4 van de Meetcode elektriciteiten bijlage 3 van de Meetcode gas RNBdoor de gezamenlijke netbeheerders is aangewezen om de meterplaatsers te erkennen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:4c89446f-8c37-5424-bc12-49b3129d1bb2" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:99
  a                 skos:Concept ;
  skos:prefLabel    "Mutatiedatum"@nl ;
  skos:definition   "De datum waarop door de netbeheerder de wijziging van stamgegevens in het aansluitingenregister wordt geëffectueerd;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:fc9644e2-eee5-5736-96e7-f4ace97181ad" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:100
  a                 skos:Concept ;
  skos:prefLabel    "MV-erkenner"@nl ;
  skos:definition   "De instantie die conform bijlage 4 van de Meetcode elektriciteiten bijlage 3 van de Meetcode gas RNBis aangewezen om de meetverantwoordelijken te erkennen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:04c866a6-29c4-5d4f-8f6b-b7e886eebfc2" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:101
  a                 skos:Concept ;
  skos:prefLabel    "MV-register"@nl ;
  skos:definition   "Een register waarin de namen, adressen, telefoon- en faxnummers alsmede de gegevens ten behoeve van elektronische gegevensuitwisseling zijn vermeld van de op grond van B4.2.1.1 van de Meetcode elektriciteiten B3.2.1.1 van de Meetcode gas RNBerkende meetverantwoordelijken;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:01c39a63-325f-57f9-8281-d063a94d8f63" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:102
  a                 skos:Concept ;
  skos:prefLabel    "Nachttarief"@nl ;
  skos:definition   ""@nl ;
  owl:versionInfo   "1" ;
  owl:deprecated    true ;
  dct:identifier    "urn:uuid:dbadf9f6-d77d-5343-b9c6-cc42b3079cf0" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:103
  a                 skos:Concept ;
  skos:prefLabel    "Netaansluiting"@nl ;
  skos:definition   ""@nl ;
  owl:versionInfo   "1" ;
  owl:deprecated    true ;
  dct:identifier    "urn:uuid:50215e59-88e1-54e4-913e-3143025ddf22" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:104
  a                 skos:Concept ;
  skos:prefLabel    "Netaansluitpunt"@nl ;
  skos:definition   "Het fysieke punt dat de grens tussen de aansluiting en het net van de netbeheerder aangeeft;"@nl ;
  owl:versionInfo   "1" ;
  skos:related      :126 ;
  dct:identifier    "urn:uuid:46b9c500-89bf-54d7-9611-21e96c8e56f5" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:105
  a                 skos:Concept ;
  skos:prefLabel    "Netbeheerdersregister"@nl ;
  skos:definition   "Administratie van bedrijfsgegevens en operationele contactgegevens van de netbeheerders in Nederland;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:25a9a0e9-aae4-5663-ad36-273d7153611a" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:106
  a                 skos:Concept ;
  skos:prefLabel    "Netgebied"@nl ;
  skos:definition   "Een aaneengesloten en samenhangend deel van een net van één netbeheerder, waarbinnen de processen termlocatie en reconciliatie plaatsvinden;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:2102b3e9-b622-50f5-8413-9aeeef4fbb1e" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:107
  a                 skos:Concept ;
  skos:prefLabel    "Nettarief"@nl ;
  skos:definition   "De door de aangeslotene verschuldigde bedragen als bedoeld in artikel 95cb van de Elektriciteitswet 1998en artikel 44b van de Gaswet;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:bff76378-e6fb-55a2-8b67-5a02c3d715ba" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:108
  a                 skos:Concept ;
  skos:prefLabel    "Netverliezen (NV)"@nl ;
  skos:definition   "Saldo van energie ingevoed op en energie afgenomen van een bepaald net gedurende een bepaalde tijd;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:d4812d53-3cb9-5935-a161-2f624576b472" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:109
  a                 skos:Concept ;
  skos:prefLabel    "Netvlak"@nl ;
  skos:definition   "Een deel van het net waarvoor geldt dat de verbruikers aangesloten op dit deel van het net eenzelfde tarief in rekening krijgen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:57fe7a39-8293-5f91-8167-36889db28c78" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:110
  a                 skos:Concept ;
  skos:prefLabel    "Niet op afstand uitleesbare kleinverbruikmeetinrichting"@nl ;
  skos:definition   "Een meetinrichting die niet voldoet aan het Besluit op afstand uitleesbare meetinrichtingen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:4b1b2798-e0cb-529d-a613-4968e1efef92" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:111
  a                 skos:Concept ;
  skos:prefLabel    "Nieuwe [leverancier / BRP / meetverantwoordelijke]"@nl ;
  skos:definition   "De [leverancier / BRP / meetverantwoordelijke] die voor een bepaalde aansluiting de rechten en plichten van een [leverancier / BRP / meetverantwoordelijke] zal gaan uitoefenen en die zich als zodanig in het aansluitingenregister heeft aangemeld of zal gaan aanmelden;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:b47bcc6d-210c-53c2-ab8e-edd0c3b83862" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:112
  a                 skos:Concept ;
  skos:prefLabel    "Nominaal meetvermogen"@nl ;
  skos:definition   "Het schijnbaar vermogen, overeenkomstig het bepaalde in B3.1.1.2.1 tot en met B3.1.1.2.4 van de Meetcode elektriciteit, te berekenen uit de nominale primaire spanning van de spanningstransformatoren en de nominale primaire stroom van de stroomtransformatoren;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:3e0047cd-8ec5-5abe-8870-262d448614bf" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:113
  a                 skos:Concept ;
  skos:prefLabel    "Normatermtarief"@nl ;
  skos:definition   ""@nl ;
  owl:versionInfo   "1" ;
  owl:deprecated    true ;
  dct:identifier    "urn:uuid:741ed8df-dd92-5c57-bbe9-66183df39702" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:114
  a                 skos:Concept ;
  skos:prefLabel    "Normaal telwerk"@nl ;
  skos:definition   "Een telwerk van een kleinverbruikmeetinrichting met twee of meer telwerken dat gebruikt wordt of kan worden voor de bepaling van de met het net uitgewisselde energie tijdens normaaluren;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:39976504-8d6d-5cf5-b1fa-be03eb4118d1" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:115
  a                 skos:Concept ;
  skos:prefLabel    "Normaaluren"@nl ;
  skos:definition   "Uren van de dag waarvoor, in geval van een meetinrichting met dubbel telwerk, de met het net uitgewisselde elektrische energie wordt geregistreerd door het normaaltelwerk. Deze periode wordt ook wel aangeduid als “peak”;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:886b1db6-9905-56e9-89d8-ffeb3e58273d" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:116
  a                 skos:Concept ;
  skos:prefLabel    "OBIS-code"@nl ;
  skos:definition   "Een code volgens het in IEC 62056-61 vastgelegde OBject Identification System, waarmee elementen van meetapparatuur kunnen worden geïdentificeerd;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:dd2f62bd-bf99-5123-9cac-72adea7ee8c3" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:117
  a                 skos:Concept ;
  skos:prefLabel    "Onbalansprijszone Nederland"@nl ;
  skos:definition   "Het gebied als bedoeld in artikel 2, dertiende lid, van de Verordening (EU) 2017/2195 (GL EB) voor Nederland, omvattende het geografische gebied van Nederland en de Nederlandse exclusieve economische zone (en valt dus samen met de biedzone Nederland);"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:ad842332-b688-5467-8b0c-f7240392d537" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:118
  a                 skos:Concept ;
  skos:prefLabel    "Onbalansverrekeningsperiode"@nl ;
  skos:definition   "De tijdseenheid waarmee de onbalansverrekening plaatsvindt, te weten 15 minuten, ofwel per klokkwartier;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:8a64b54b-4ab9-5e14-820b-02981cd8e8c0" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:119
  a                 skos:Concept ;
  skos:prefLabel    "Onbalanszone Nederland"@nl ;
  skos:definition   "Het gebied als bedoeld in artikel 2, elfde lid, van de Verordening (EU) 2017/2195 (GL EB) voor Nederland, omvattende het geografische gebied van Nederland en de Nederlandse exclusieve economische zone (en valt dus samen met de biedzone Nederland);"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:60dffd4b-42b2-5548-a982-0ae87d1beae7" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:120
  a                 skos:Concept ;
  skos:prefLabel    "Onbemeten aansluiting"@nl ;
  skos:definition   "Een aansluiting waarbij op grond van artikel 2.30of 2.31 van de Netcode elektriciteitgeen meetinrichting aanwezig is;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:6b9c91a5-313c-5e0f-90ed-fdc2895df593" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:121
  a                 skos:Concept ;
  skos:prefLabel    "Op afstand uitleesbare kleinverbruikmeetinrichting"@nl ;
  skos:definition   "Een meetinrichting die voldoet aan het Besluit op afstand uitleesbare meetinrichtingen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:dddab8f2-cf39-5b02-977d-1ce08532ff1b" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:122
  a                 skos:Concept ;
  skos:prefLabel    "Opgesteld vermogen"@nl ;
  skos:definition   "Het maximale vermogen van een elektriciteitsproductie-eenheid dat onder nominale condities benut kan worden voor het leveren van elektrische energie (nameplate capacity)."@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:52d505f0-4c64-5e6a-8e6a-6f01010358d8" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:123
  a                 skos:Concept ;
  skos:prefLabel    "Opwekvermogen"@nl ;
  skos:definition   ""@nl ;
  owl:versionInfo   "1" ;
  owl:deprecated    true ;
  dct:identifier    "urn:uuid:1c3fa699-7c04-5ea0-9064-e314043fa79f" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:124
  a                 skos:Concept ;
  skos:prefLabel    "Oude [leverancier / BRP / meetverantwoordelijke]"@nl ;
  skos:definition   "De [leverancier / BRP / meetverantwoordelijke] die voor een bepaalde aansluiting de rechten en plichten zal gaan verliezen of heeft verloren en als gevolg daarvan uit het aansluitingenregister is verwijderd of zal worden verwijderd;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:c77ea082-3091-5b2e-a649-2d03ccab149a" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:125
  a                 skos:Concept ;
  skos:prefLabel    "Overdrachtspunt"@nl ;
  skos:definition   "Het fysieke punt waar respectievelijk een scheiding tussen de aansluiting van twee netten onderling of tussen de aansluiting van een net en de installatie van de aangeslotene kan worden gerealiseerd;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:7b4e6150-01a4-5ce9-a783-0b4a3e0fa89d" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:126
  a                 skos:Concept ;
  skos:prefLabel    "P4"@nl ;
  skos:definition   "Onderdeel van de op afstand uitleesbare kleinverbruikmeetinrichting door middel waarvan de netbeheerder en de leverancier en in geval van een door de aangeslotene verstrekte machtiging een derde, toegang hebben tot de meetgegevens van die kleinverbruikmeetinrichting;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:b60d6ce0-96a5-57f7-8874-4fa0b0be9a06" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:127
  a                 skos:Concept ;
  skos:prefLabel    "Particulier net"@nl ;
  skos:definition   ""@nl ;
  owl:versionInfo   "1" ;
  owl:deprecated    true ;
  dct:identifier    "urn:uuid:99ea499a-2a91-59cb-8703-77f2ec2dfb83" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:128
  a                 skos:Concept ;
  skos:prefLabel    "Per unit (pu)"@nl ;
  skos:definition   "De referentiespanning die in een laagspanningsnet gelijk is aan de spanning U nte weten 230 V, in een middenspanningsnet gelijk is aan de spanning U c, in een hoogspanningsnet met een spanningsniveau kleiner dan of gelijk aan 220 kV gelijk is aan spanning U c, en in een hoogspanningsnet met een spanningsniveau groter dan 220 kV gelijk is aan de spanning U n;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:cbd02a7f-c594-5e7c-bd86-9a36d8b9628a" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:129
  a                 skos:Concept ;
  skos:prefLabel    "Primair allocatiepunt"@nl ;
  skos:definition   "Het eerste aan een aansluiting toegekende allocatiepunt;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:7d952547-c482-5e8d-8e76-3238fec714e0" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:130
  a                 skos:Concept ;
  skos:prefLabel    "Primair gedeelte van de meetinrichting"@nl ;
  skos:definition   "De eventueel aanwezige spannings- en stroommeettransformatoren met inbegrip van de aansluitklemmen waarop het secundaire deel van de meetinrichting is aangesloten;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:a60e5137-8a00-56f8-a7f3-bdbdd66a44b8" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:131
  a                 skos:Concept ;
  skos:prefLabel    "Primaire energiebron"@nl ;
  skos:definition   "Een aanduiding voor de soort brandstof en/of energiebron voor de elektriciteitsproductie-eenheid, onderverdeeld in de volgende types: zon; wind; water; biomassa; kolen; gas; olie; nucleair; overig;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:441f7af4-a2de-57c7-a91d-203f94bfc3f4" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:132
  a                 skos:Concept ;
  skos:prefLabel    "Productiemeetinrichting"@nl ;
  skos:definition   "Een meetinrichting waarmee de hoeveelheid opgewekte elektrische energie op de plaats waar de GCvO-installatie met de rest van de elektrische installatie verbonden is wordt gemeten;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:e3350059-245c-5d9b-962e-15e25988dd8f" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:133
  a                 skos:Concept ;
  skos:prefLabel    "Profielcategorie (PC)"@nl ;
  skos:definition   "Een classificering van aansluitingen op grond van objectief vast te stellen kenmerken, zoals aansluitwaarde, schakeltijden, gecontracteerd transportvermogen, vastgesteld afnametype en bedrijfstijd;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:fec5703a-1379-5f5a-a2d8-081b9b880815" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:134
  a                 skos:Concept ;
  skos:prefLabel    "Profielgrootverbruikaansluiting"@nl ;
  skos:definition   "Een grootverbruikaansluiting waarbij een profielgrootverbruikmeetinrichting aanwezig is;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:eaf0fb43-adbd-5a03-b193-0a56f26e5cae" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:135
  a                 skos:Concept ;
  skos:prefLabel    "Profielgrootverbruikmeetinrichting"@nl ;
  skos:definition   "Een meetinrichting die voldoet aan de in 4.3.4 van de Meetcode elektriciteitgenoemde eisen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:031fb7ea-d782-5558-84cd-76a670f16784" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:136
  a                 skos:Concept ;
  skos:prefLabel    "Prognose"@nl ;
  skos:definition   "Een door de aangeslotene of diens BRP per tijdsperiode opgestelde en bij de betreffende netbeheerder ingediende planning van de som van afname of invoeding per primaire energiebron per overdrachtspunt van de aansluiting of verzamelpunt dat valt onder de verantwoordelijkheid van de aangeslotene of diens BRP;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:e0787fe7-1ba4-5c3e-8cc3-3f845a261b29" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:137
  a                 skos:Concept ;
  skos:prefLabel    "Programmeringszone Nederland"@nl ;
  skos:definition   "Het gebied als bedoeld in artikel 3, tweede lid, onderdeel 91, van de Verordening (EU) 2017/type (GL SO) voor Nederland, omvattende het geografische gebied van Nederland en de Nederlandse exclusieve economische zone (en valt dus samen met de biedzone Nederland);"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:9c97da91-0795-562b-8c92-cf05ce2af799" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:138
  a                 skos:Concept ;
  skos:prefLabel    "Raad van bestuur van de mededingingsautoriteit"@nl ;
  skos:definition   ""@nl ;
  owl:versionInfo   "1" ;
  owl:deprecated    true ;
