:Location a owl:Class,
        skos:Concept ;
    skos:prefLabel "Location"@en ;
    skos:definition "The place, scene, or point of something where someone or something has been, is, and/or will be at a given moment in time. It can be defined with one or more position points (coordinates) in a given coordinate system."@en ;
    owl:versionInfo "3.0" ;
    cims:belongsToCategory "Package_GeographicalLocationProfile" ;
    dct:created "2025-08-04"^^xsd:date ;
