  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:80
  a                 skos:Concept ;
  skos:prefLabel    "kWmax"@nl ;
  skos:definition   "De werkelijk voorgekomen maximale belasting van het net door een aangeslotene op een netaansluiting, die bij een meetperiode van 15 minuten of minder wordt bepaald op basis van de waarde per klokkwartier en die bij een andere meetperiode wordt bepaald op basis van de hoogste waarde per meetperiode;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:afc03ecb-ad11-5ce1-99e9-7e88ab006296" ;
