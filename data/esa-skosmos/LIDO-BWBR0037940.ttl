  dct:identifier    "urn:uuid:a9f4f7c2-5668-5e11-a2f7-7c0a862ddde1" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:170
  a                 skos:Concept ;
  skos:prefLabel    "Standaardjaarinvoeding (SJI)"@nl ;
  skos:definition   "De verwachte invoeding op een aansluiting bij gestandaardiseerde condities en op basis van een genormaliseerd jaar;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:4cbdb796-eaa1-5f35-9a49-a52025237e92" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:171
  a                 skos:Concept ;
  skos:prefLabel    "Standaardprofiel"@nl ;
  skos:definition   "Een vooraf vastgestelde tijdreeks van profielfracties, voor een bepaalde profielcategorie;"@nl ;
