  dct:identifier    "83ad2b09-8d7c-5071-9bdb-e1c91acfe5a4" ;
  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

:2258
  a                 skos:Concept ;
  skos:prefLabel    "Productblad"@nl ;
  skos:definition   "Het productblad vormt een onderdeel van de Service Level Agreement (SLA) tussen AM en Infra Services. Het betreft een offerte voor het leveren van een product, door interne leverancier (Nuon-Infra) opgesteld document waarin de door Nuon gedefinieerde producten zijn voorzien van gegevens zoals omschrijving, leverende afdeling, verrekeneenheid, tarief, rapportage(s), etc."@nl ;
  owl:versionInfo   "1" ;
  dct:created       "2025-07-13"^^xsd:date ;
  dct:identifier    "2891945d-d3e9-55c4-a39e-1582f9f6f432" ;
  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

