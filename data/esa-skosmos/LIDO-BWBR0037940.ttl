  dct:identifier    "urn:uuid:4bb67e07-0824-5520-9f63-8b66712612cc" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:172
  a                 skos:Concept ;
  skos:prefLabel    "Stroomsommatie (van de meetinrichting)"@nl ;
  skos:definition   "Het sommeren van meetstromen door parallelschakeling van de secundaire wikkelingen van stroomtransformatoren of door toepassing van sommeerstroomtransformatoren;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:cb4e5e1e-0e17-54d6-864a-403e9af826b1" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:173
  a                 skos:Concept ;
  skos:prefLabel    "Switch"@nl ;
  skos:definition   "De wisseling van leverancier, BRP of meetverantwoordelijke op een aansluiting;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:588db38c-8745-5d28-b808-3dbcf996f460" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:174
  a                 skos:Concept ;
  skos:prefLabel    "Syntactisch correct"@nl ;
  skos:definition   "Voldoet aan de specificaties, bedoeld in 9.1.1 van de Informatiecode elektriciteit en gas;"@nl ;
