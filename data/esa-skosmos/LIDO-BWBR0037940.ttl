:154
  a                 skos:Concept ;
  skos:prefLabel    "Restantvolume"@nl ;
  skos:definition   "Het volume dat nog niet toegewezen is, na toewijzing van gemeten afnames en invoedingen, berekende afnames en invoedingen en het netverlies."@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:a48e8389-5a6b-554c-ade2-a15b41412c51" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:155
  a                 skos:Concept ;
  skos:prefLabel    "Restantvolumecorrectiefactor (RCF)"@nl ;
