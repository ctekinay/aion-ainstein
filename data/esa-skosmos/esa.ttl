  skos:prefLabel    "Activation Term"@en ;
  skos:definition   "Activation is the execution of a contracted flexibility action, whereby an eligible resource increases or decreases demand or generation in response to an operational constrain, within agreed volume, timing, duration, and verification conditions, to support system balance, congestion relief, or security of supply."@en ;
  skos:relatedMatch :36, :97 ;
  dct:identifier    "urn:uuid:ef341459-1dc4-419a-827a-a164988aef34" ;
