  dct:identifier    "urn:uuid:662f501f-e786-545d-b60e-aba18116a1d7" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:175
  a                 skos:Concept ;
  skos:prefLabel    "Systeemdienstentarief"@nl ;
  skos:definition   ""@nl ;
  owl:versionInfo   "1" ;
  owl:deprecated    true ;
  dct:identifier    "urn:uuid:9daf9bd0-090f-5f04-81d6-2add2d02be44" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:176
  a                 skos:Concept ;
  skos:prefLabel    "System operator"@nl ;
  skos:definition   "De netbeheerder van het landelijk hoogspanningsnet, waar deze verantwoordelijke is voor het leveren van de systeemdiensten;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:9e398af8-886b-5402-b1cf-adcbd10b4a36" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:177
  a                 skos:Concept ;
  skos:prefLabel    "Tariefcategorie (TC)"@nl ;
  skos:definition   "Eén van de categorieën volgens 3.7.1 van de Tarievencode elektriciteit, waarbij binnen categorie f: “LS” nog worden onderscheiden de categorieën van aangeslotenen met een meetinrichting met enkeltelwerk, aangeslotenen met een meetinrichting met dubbel telwerk die omstreeks 21:00 uur omschakelt van normaal- of totaaltelwerk naar laagtelwerk en aangeslotenen met een meetinrichting met dubbel telwerk die omstreeks 23:00 uur omschakelt van normaal- of totaaltelwerk naar laagtelwerk;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:1a773924-9f8d-56e0-b863-1ea8a4b4dd0e" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:178
  a                 skos:Concept ;
  skos:prefLabel    "Tariefdrager"@nl ;
  skos:definition   "Een element om de aan een deelmarkt toegedeelde kosten bij de aan het netvlak aangesloten verbruikers in rekening te brengen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:b7c64f77-5885-5ba4-a185-70b276efa81e" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:179
  a                 skos:Concept ;
  skos:prefLabel    "Tariefperiode (TP)"@nl ;
  skos:definition   "Een aaneengesloten periode van normaaluren of laaguren;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:57bfa5cf-993a-5db9-ae95-748682b1acb7" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:180
  a                 skos:Concept ;
  skos:prefLabel    "Tariefzone"@nl ;
  skos:definition   "De aanduiding in het aansluitingenregister van het kenmerk van een telwerk, te weten normaaltelwerk, totaaltelwerk of laagtelwerk. Ingeval van een enkeltelwerk wordt altijd de aanduiding normaaltelwerk gebruikt;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:6c57f7a1-8118-524d-8ddb-bb76df24c996" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:181
  a                 skos:Concept ;
  skos:prefLabel    "Telemetriegrootverbruikaansluiting"@nl ;
  skos:definition   "Een grootverbruikaansluiting die geen profielgrootverbruikaansluiting is;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:528a68da-9a74-5b07-91a8-04ce7cd88969" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:182
  a                 skos:Concept ;
  skos:prefLabel    "Telemetriegrootverbruikmeetinrichting"@nl ;
  skos:definition   "Een meetinrichting die voldoet aan de in 4.3.5 van de Meetcode elektriciteitgenoemde eisen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:f0e5dcc1-0509-5a2b-b692-c762ca627a66" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:183
  a                 skos:Concept ;
  skos:prefLabel    "Telwerk"@nl ;
  skos:definition   "Een samenstel van cijferrollen bij een Ferrarismeter dan wel een dataregister van een digitale kWh-meter waarin de gemeten totale hoeveelheid elektrische energie voor een bepaalde energierichting voor normaaluren of laaguren wordt vastgelegd;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:9794e73a-ca49-503b-99a4-809db317fa6f" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:184
  a                 skos:Concept ;
  skos:prefLabel    "Telwerkindicatie"@nl ;
  skos:definition   "De identificatie van de onderscheiden telwerken op een meetinrichting: - Bij een handopgenomen meetinrichting zoals zichtbaar op de meter zelf, en; - Bij een meetinrichting die op afstand uitleesbaar is door middel van de OBIS-code;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:2eb7aeae-ceb0-554f-8563-908a7736bbc9" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:185
  a                 skos:Concept ;
  skos:prefLabel    "Toegankelijk meetregister"@nl ;
  skos:definition   "Een register, bevattende vastgestelde meterstanden en verbruiken betreffende kleinverbruikaansluitingen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:f3ad70b3-7e89-504d-baf0-17f0b26a617d" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:186
  a                 skos:Concept ;
  skos:prefLabel    "Toestemmingssleutel"@nl ;
  skos:definition   "een door de leverancier toegekend uniek kenmerk aan de door hem van de klant ontvangen toestemming;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:cd34f4b2-2a9d-567e-819d-9b0d265bf27d" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:187
  a                 skos:Concept ;
  skos:prefLabel    "Totaal verondersteld geprofileerd volume (TVGV)"@nl ;
  skos:definition   "De som van de absolute waarde van termle veronderstelde geprofileerde afnames en invoedingen in het net van de netbeheerder;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:0fe5944e-7db3-5c29-800c-42d8a7bb6e76" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:188
  a                 skos:Concept ;
  skos:prefLabel    "Totaaltelwerk"@nl ;
  skos:definition   "Een telwerk van een kleinverbruikmeetinrichting met twee of meer telwerken dat gebruikt wordt of kan worden voor de bepaling van de met het net uitgewisselde energie tijdens normaal- en laaguren tezamen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:f8a631f9-f0d3-50c3-9aae-e3d1ab7e56b7" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:189
  a                 skos:Concept ;
  skos:prefLabel    "Transactiepartij"@nl ;
  skos:definition   "Een natuurlijke of rechtspersoon die in het energieprogramma van een BRP als bedoeld in artikel 10.2 van de Netcode elektriciteitals partij is vermeld op de grond dat die BRP in dat programma een energietransactie met die natuurlijke of rechtspersoon verantwoordt;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:64531e69-ae72-5377-9b3a-c18a413ae628" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:190
  a                 skos:Concept ;
  skos:prefLabel    "Transit"@nl ;
  skos:definition   "Elektriciteitstransport door een land als gevolg van een contract tussen partijen in twee andere landen, uitgevoerd door de netbeheerder van het landelijk hoogspanningsnet;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:8d08ded0-1597-5648-972e-10cd9df54374" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:191
  a                 skos:Concept ;
  skos:prefLabel    "Transportdienst"@nl ;
  skos:definition   "Het transporteren van elektriciteit van producenten naar verbruikers door gebruik te maken van het net. Hieronder wordt mede verstaan het oplossen van fysieke congestie, het compenseren van netverliezen en het in stand houden van de spannings- en blindvermogenshuishouding;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:26ba89f8-6633-5d0b-8f5b-eb606500c9cc" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:192
  a                 skos:Concept ;
  skos:prefLabel    "Transporttarief"@nl ;
  skos:definition   "Het tarief in rekening gebracht door de netbeheerder bij een aangeslotene ter dekking van de kosten van de transportdienst;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:4649b49b-c435-515a-bdca-4e8d6cb539f5" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:193
  a                 skos:Concept ;
  skos:prefLabel    "Type aansluiting"@nl ;
  skos:definition   """De aanduiding in het aansluitingenregister of de aansluiting geschikt is voor:

  owl:versionInfo   "1" ;
  - Uitsluitend transport van elektriciteit of gas vanuit het net naar de aangesloten installatie, of;
  - Uitsluitend transport van elektriciteit of gas vanuit de aangesloten installatie naar het net, of;
  - Een combinatie van beide;"""@nl ;
  dct:identifier    "urn:uuid:a15ebd33-9825-59f6-97b8-dd8bb486aab4" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:194
  a                 skos:Concept ;
  skos:prefLabel    "Uitgelezen meterstand"@nl ;
  skos:definition   "Een meterstand die met behulp van elektronische hulpmiddelen ter plaatse van de meetinrichting of op afstand is uitgelezen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:9ac92520-c492-58bf-a278-c1e89fa1b86b" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:195
  a                 skos:Concept ;
  skos:prefLabel    "Uithuizing"@nl ;
  skos:definition   "Een verhuizing waarbij een (rechts)persoon een onroerende zaak verlaat en de beschikkingsbevoegdheid over de aansluiting beëindigt;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:3932ebbc-d2e5-58a5-9458-20ff3e19bc3d" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:196
  a                 skos:Concept ;
  skos:prefLabel    "Uit-switch"@nl ;
  skos:definition   ""@nl ;
  owl:versionInfo   "1" ;
  owl:deprecated    true ;
  dct:identifier    "urn:uuid:67ed786b-2324-5876-b545-eb11a2100372" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:197
  a                 skos:Concept ;
  skos:prefLabel    "Uitwisseling met het buitenland"@nl ;
  skos:definition   "De som van alle importen uit het buitenland van elektrische energie verminderd met de som van alle exporten naar het buitenland van elektrische energie;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:0f13253b-d452-58d2-969e-7f50f8ced2f5" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:198
  a                 skos:Concept ;
  skos:prefLabel    "Validatiestatus"@nl ;
  skos:definition   "Het resultaat van de datavalidatie door de meetverantwoordelijke van de betreffende meetgegevens;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:c158c189-65c5-56f1-bb4e-c9d374f95110" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:199
  a                 skos:Concept ;
  skos:prefLabel    "Vastgesteld afnametype"@nl ;
  skos:definition   "De bepaling per aansluiting of sprake is van afname met invoeding of afname zonder invoeding;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:4aa38ba9-f4cd-5e31-89fc-910a5558c387" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:200
  a                 skos:Concept ;
  skos:prefLabel    "Vastgestelde meterstand"@nl ;
  skos:definition   "Een meterstand die na collectie en succesvolle validatie is vastgesteld door: - Het meetbedrijf (ingeval deze meterstand betrekking heeft op een kleinverbruikmeetinrichting) of - De meetverantwoordelijke (ingeval deze meterstand betrekking heeft op een grootverbruikmeetinrichting);"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:c9c86096-0dd9-58d7-97af-bb84be96d57b" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:201
  a                 skos:Concept ;
  skos:prefLabel    "Verbruiker"@nl ;
  skos:definition   "Een aangeslotene die elektrische energie afneemt van het net;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:2c050b09-f40d-58e1-9d20-436904c58042" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:202
  a                 skos:Concept ;
  skos:prefLabel    "Verliesbericht"@nl ;
  skos:definition   "Het bericht waarmee de netbeheerder aan de oude leverancier, oude BRP of oude meetverantwoordelijke aankondigt dat de desbetreffende marktpartij met ingang van de datum in het verliesbericht de rechten en plichten voor de in het verliesbericht vermelde aansluiting zal verliezen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:1276e2d5-a443-53d5-b7c8-f76a4ae4990c" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:203
  a                 skos:Concept ;
  skos:prefLabel    "Vermenigvuldigingsfactor"@nl ;
  skos:definition   "In geval van elektriciteit het samenstel van alle correctiefactoren met betrekking tot de fysieke meting die nodig zijn om uit de tellerstanden de daadwerkelijke hoeveelheid met het net uitgewisselde energie te kunnen vaststellen; In geval van gas het product van alle specifiek voor de meetinrichting van toepassing zijnde factoren op telwerkniveau met uitzondering van de volumeherleidingsfactor om tot een normaalvolume te komen;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:39eecef6-ae25-5edd-9afa-ea69dd6fc2f1" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:204
  a                 skos:Concept ;
  skos:prefLabel    "Verondersteld geprofileerde afname (VGA)"@nl ;
  skos:definition   "De profielfractie voor afname (PFA) vermenigvuldigd met de som van de standaardjaarafnames van ale allocatiepunten per BRP per leverancier per netgebied in een profielcategorie;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:c3714d50-a293-5f25-b623-cb70472fe523" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:205
  a                 skos:Concept ;
  skos:prefLabel    "Verondersteld geprofileerde invoeding (VGI)"@nl ;
  skos:definition   "De profielfractie voor invoeding (PFI) vermenigvuldigd met de som van de standaardjaarinvoedingen van alle allocatiepunten per BRP per leverancier per netgebied in een profielcategorie;"@nl ;
