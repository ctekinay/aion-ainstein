:61
  a               skos:Concept ;
  skos:prefLabel  "Interconnectorsysteembeheerder voor gas"@nl ;
  skos:definition "Rechtspersoon die op grond van artikel 3.2, eerste lid, onderdeel d, is aangewezen"@nl ;
  dct:identifier  "urn:uuid:3eb5e7dc-a60e-4dde-96d9-fda5aca067ea" ;
  skos:broader    :59 ;
