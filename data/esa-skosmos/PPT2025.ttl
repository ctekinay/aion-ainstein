:642
  a                 skos:Concept ;
  skos:prefLabel    "Data modellering & design"@nl ;
  skos:definition   "managet data vereisten en de vastlegging daarvan in een datamodel. Dit proces is iteratief en kan een conceptueel, logisch en fysiek model opleveren dat gekoppeld kan worden met de data-architectuur."@nl ;
  owl:versionInfo   "1" ;
  dct:created       "2025-07-13"^^xsd:date ;
  dct:identifier    "c05bd9a3-9853-5bbc-9f26-15263d3f5ff8" ;
  skos:inScheme     :Ontology ;
  skos:topConceptOf :Ontology .

:643
  a                 skos:Concept ;
  skos:prefLabel    "Dataobject"@nl ;
