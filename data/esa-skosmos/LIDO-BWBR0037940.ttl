:153
  a                 skos:Concept ;
  skos:prefLabel    "Reparatiemethodiek"@nl ;
  skos:definition   "De wijze waarop de meetverantwoordelijke of netbeheerder meetgegevens heeft gecorrigeerd of berekend;"@nl ;
