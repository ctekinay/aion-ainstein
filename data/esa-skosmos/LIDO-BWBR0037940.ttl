  dct:identifier    "urn:uuid:1390ae09-fa6c-57f3-afd0-5f7b4fa8554a" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

:162
  a                 skos:Concept ;
  skos:prefLabel    "Secundair gedeelte van de meetinrichting"@nl ;
  skos:definition   "Het deel van de meetinrichting vanaf de aansluitklemmen van de secundaire wikkelingen van de meettransformatoren tot en met de kWh-meters en de kvarh-meters en de dagelijks op afstand uitleesbare meetinrichting;"@nl ;
  owl:versionInfo   "1" ;
  dct:identifier    "urn:uuid:62a422a8-f8af-564f-b5a8-28153f2007bc" ;
  skos:topConceptOf :Ontology ;
  skos:inScheme     :Ontology .

